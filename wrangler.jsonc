{
	"main": "dist/_worker.js/index.js",
	"name": "peppos",
  "compatibility_date": "2026-02-07",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "shop.example.com",
      "custom_domain": true
    }
  ]
}

